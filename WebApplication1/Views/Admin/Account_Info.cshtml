@model WebApplication1.Models.Admin

@{
    ViewBag.Title = "Account_Info";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Account Info</h2>

<div>
    <h4>Admin @Html.DisplayFor(model => model.ad_name)</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            <strong>Email</strong>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.email)
        </dd>

        <dt>
            <strong>Password</strong>
        </dt>

        <dd>
            @Html.DisplayFor(model => model.password)
        </dd>
    </dl>
</div>
<div>
    <a class="btn btn-danger" href="@Url.Action("Index", "Admin")">Back</a> |
    <a class="btn btn-danger" href="@Url.Action("LogOut", "User")">LogOut</a>
</div>